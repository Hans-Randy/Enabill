@model List<Enabill.Models.Dto.NoteDetailModel>

    <table class="calendarTable fullWidth">
        <tr>
            <th>
                Project
            </th>
            <th>
                Activity
            </th>
            <th>
                Date
            </th>
            <th>
            </th>
        </tr>
        @foreach (var note in Model)
        {
            <tr>
                <td>
                    @note.Project.ProjectName
                </td>
                <td>
                    @note.Activity.ActivityName
                </td>
                <td>
                    @note.WorkAllocation.DayWorked.ToDisplayString()
                </td>
                <td>
                    <a href="#" onclick="Note.view(@note.WorkAllocation.WorkAllocationID); return false;">View</a>
                </td>
            </tr>
        }
    </table>