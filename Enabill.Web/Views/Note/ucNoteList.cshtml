@model List<Enabill.Models.Dto.NoteDetailModel>

    <table class="subTable fullWidth">
        <tr>
            <td>

                @if (Model.Count() == 0)
            {
                    <br />
                    <h1 style="display: inline-block; width: 100%;" class="center">
                        There are no notes to display for search criteria
                    </h1>
                }
                @foreach (var note in Model)
            {
                    <div id="Note@(note.Note.NoteID)">
                        @Html.Partial("ucNoteDetail", note)
                    </div>
                }
            </td>
        </tr>
    </table>