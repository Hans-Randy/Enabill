@model string

<div class="padView">
    <form id="frmExpenseReport" method="post">
        <table>
            <tr>
                <th>Date From</th>
                <td>@Html.TextBox("DateFrom", DateTime.Today.ToFirstDayOfMonth().ToDisplayString())</td>
            </tr>
            <tr>
                <th>Date To</th>
                <td>@Html.TextBox("DateTo", DateTime.Today.ToLastDayOfMonth().ToDisplayString())</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>@Html.DropDownList("ExpenseActiveUserList")</td>
            </tr>
            <tr>
                <th>Employment Type</th>
                <td>@Html.DropDownList("ExpenseEmploymentTypeList")</td>
            </tr>
            <tr>
                <th>Employees</th>
                <td>@Html.DropDownList("ExpenseEmployeeList")</td>
            </tr>
            <tr>
                <th>Clients</th>
                <td>@Html.DropDownList("ExpenseClientList")</td>
            </tr>
            <tr>
                <th>Projects</th>
                <td>@Html.DropDownList("ExpenseProjectList")</td>
            </tr>
            <tr>
                <th>Expense Category Types</th>
                <td>@Html.DropDownList("ExpenseCategoryTypeList")</td>
            </tr>
            <tr>
                <th>Approval Status</th>
                <td>@Html.DropDownList("ExpenseApprovalList")</td>
            </tr>
            <tr>
                <th>Billable</th>
                <td>@Html.DropDownList("ExpenseBillableList")</td>
            </tr>
        </table>
    </form>
</div>